@model IEnumerable<LibraryManagementSystem.Models.Publisher>

@{
    ViewBag.Title = "Index";
}

<h2>Publishers</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Name of Publishers
            @*@Html.DisplayNameFor(model => model.NameOfPublisher)*@
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NameOfPublisher)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PublisherID_PK }) |
            @Html.ActionLink("Details", "Details", new { id=item.PublisherID_PK }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PublisherID_PK })
        </td>
    </tr>
}

</table>
